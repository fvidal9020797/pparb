	<subDataset name="UsoActual" uuid="d0cf26ef-68ec-4b0f-9082-bff8e925b076">
		<parameter name="COD_PAR1" class="java.lang.String"/>
		<queryString>
			<![CDATA[select s.*, r.idparcela
from Superficiealimentos as s inner join registro as r on s.idregistro = r.idregistro
