	<subDataset name="funcionarioABT" uuid="1e62e6ab-3fe3-48b5-aaab-5b4fe232cd1f">
		<parameter name="ID_REG" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select nomcompleto as abt, idregistro
from funcionarioregistro as fr inner join funcionario as r on fr.idfuncionario = r.idfuncionario inner join v_persona as v on r.idpersona = v.idpersona
where idregistro = $P{ID_REG} and tiporegistrador = 138]]>
		</queryString>
		<field name="abt" class="java.lang.String"/>
		<field name="idregistro" class="java.lang.Integer"/>
	</subDataset>
