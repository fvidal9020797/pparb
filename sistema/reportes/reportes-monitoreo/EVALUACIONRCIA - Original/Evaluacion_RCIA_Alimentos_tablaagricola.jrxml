	<subDataset name="notaagricola" uuid="51782ac7-87cf-4bb4-9cbb-e7551f843c20">
		<parameter name="idmonitoreo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select idmonitoreo, sum(puntuacion) as puntuacionagricola
from monitoreo.valoracionalimentos as mva inner join monitoreo.evaluacionespecifica as mee on mva.idevalespecif = mee.idevaluacionespecifica
inner join monitoreo.evaluaciongeneral as meg on mee.idevalgral = meg.idevalgral
where mva.tipo = 70 and idmonitoreo = $P{idmonitoreo}
group by idmonitoreo]]>
		</queryString>
		<field name="idmonitoreo" class="java.lang.Integer"/>
		<field name="puntuacionagricola" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="detallecultivo" uuid="46c9a346-803b-4569-a4c4-b38038382d4e">
		<parameter name="idmonitoreo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select va.idmonitoreo, c.nombrecultivo, dvc.puntuacioncultivo, va.puntuacion,ee.parametroespecifico, va.idtablavaloracion
from monitoreo.valoracionalimentos as va inner join monitoreo.detallevalorcultivo as dvc on va.idtablavaloracion = idtablavalorali
inner join cultivo as c on c.idcultivo = dvc.idcultivo inner join monitoreo.evaluacionespecifica as ee on ee.idevaluacionespecifica = va.idevalespecif
where va.tipo = 70   and va.idmonitoreo = $P{idmonitoreo}
order by idtablavaloracion asc]]>
		</queryString>
		<field name="idmonitoreo" class="java.lang.Integer"/>
		<field name="nombrecultivo" class="java.lang.String"/>
		<field name="puntuacioncultivo" class="java.lang.Integer"/>
		<field name="puntuacion" class="java.math.BigDecimal"/>
		<field name="parametroespecifico" class="java.lang.String"/>
		<field name="idtablavaloracion" class="java.lang.Integer"/>
	</subDataset>
