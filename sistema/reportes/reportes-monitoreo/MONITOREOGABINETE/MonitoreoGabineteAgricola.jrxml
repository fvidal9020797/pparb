	<subDataset name="puntotalsupcultivos" uuid="ed7e87c1-0762-429f-b2d4-2433aa74014f">
		<queryString language="SQL">
			<![CDATA[select sum(puntuacion)/2 as punttotalsupcultivos
from monitoreo.evaluacion_mon_gabinete_agricola as ma inner join cultivo as c on c.idcultivo = ma.idcultivo
where idmonitoreo = 2259
]]>
