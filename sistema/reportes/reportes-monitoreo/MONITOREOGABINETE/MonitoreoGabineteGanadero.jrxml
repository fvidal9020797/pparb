	<subDataset name="tablagabinete" uuid="b04107c3-71b6-46bd-bfd8-4664a7ecdb1a">
		<parameter name="paramidmonitoreo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select mg.idmonitoreo, e.parametroespecifico, mg.comprometido, mg.cuantificado, e.valoracion, mg.alcanzado, mg.fuentes from
monitoreo.evaluacion_mon_gabinete as mg inner join monitoreo.evaluacionespecifica as e on mg.idevaluacionespecifica = e.idevaluacionespecifica
where mg.idmonitoreo = $P{paramidmonitoreo}
order by mg.ideval_mon_gabiente asc]]>
		</queryString>
		<field name="idmonitoreo" class="java.lang.Integer"/>
		<field name="parametroespecifico" class="java.lang.String"/>
		<field name="comprometido" class="java.lang.Double"/>
		<field name="cuantificado" class="java.lang.Double"/>
		<field name="valoracion" class="java.lang.Integer"/>
		<field name="alcanzado" class="java.lang.Double"/>
		<field name="fuentes" class="java.lang.String"/>
		<variable name="sumaalcanzado" class="java.lang.Float" calculation="Sum">
			<variableExpression><![CDATA[$F{alcanzado}]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="notafinal" uuid="a68e3c7b-b44d-40ab-8df2-84a563763c1f">
		<parameter name="idmonpara" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select mg.idmonitoreo, SUM(mg.alcanzado) as sumaporcentaje from
monitoreo.evaluacion_mon_gabinete as mg inner join monitoreo.evaluacionespecifica as e on mg.idevaluacionespecifica = e.idevaluacionespecifica
where mg.idmonitoreo = $P{idmonpara}
group by mg.idmonitoreo]]>
		</queryString>
		<field name="idmonitoreo" class="java.lang.Integer"/>
		<field name="sumaporcentaje" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="observaciones" uuid="f59749b3-abf8-406f-bc1b-606e21b78bef">
		<parameter name="idmon" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select * from monitoreo.observacion
where idmonitoreo = $P{idmon}]]>
		</queryString>
		<field name="idmonitoreo" class="java.lang.Integer"/>
		<field name="tipo" class="java.lang.Integer"/>
		<field name="obs" class="java.lang.String"/>
		<field name="estado" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="idregistro" class="java.lang.Integer"/>
	<parameter name="idmonitoreo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select * from
(SELECT v_parcela.idregistro, v_parcela.nombre AS nombrepredio, v_parcela.idparcela, v_parcela.departamento, v_parcela.provincia, v_parcela.nombrepolitico AS municipio,
  v_parcela.clasificacionprog, v_parcela."TipoProp", v_parcela."SitJur", v_parcela."Oficina", v_parcela.estado, v_parcela.asesoramiento, v_parcela.fecharegistro,
  v_parcela.superficie AS suppredio, v.supdefilegal, v.suppas, fr.fechasuscripcion
   FROM v_parcela
   LEFT JOIN v_registro v ON v_parcela.idregistro = v.idregistro
   LEFT JOIN superficiealimentos sa ON sa.idregistro = v_parcela.idregistro
   FULL JOIN fechasregistro fr ON fr.idregistro = v_parcela.idregistro) a
inner join
(select x.idmonitoreo, x.idregistro, x.anho, x.funcionarioalimentos,x.nombrecargo, x.img_dir, x.numeroinforme, x.fechainforme from
(select m.idmonitoreo, m.idregistro, m.anho, m.numeroinforme, m.fechainforme, f.nomcompleto as funcionarioalimentos, f.nombrecargo, m.oficina, i.img_dir
from monitoreo.monitoreo as m inner join monitoreo.funcionariomonitoreo as fm
on m.idmonitoreo = fm.idmonitoreo inner join v_funcionario_general as f on f.idfuncionario = fm.idfuncionario
inner join monitoreo.imagen_monitoreo as i on m.idmonitoreo = i.idmonitoreo where tipo = 268 and tiporegistrador = 138  ) x
) b on a.idregistro = b.idregistro
where a.idregistro = $P{idregistro} and b.idmonitoreo = $P{idmonitoreo}]]>
	</queryString>
	<field name="idregistro" class="java.lang.Integer"/>
	<field name="nombrepredio" class="java.lang.String"/>
	<field name="idparcela" class="java.lang.String"/>
	<field name="departamento" class="java.lang.String"/>
