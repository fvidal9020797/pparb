	<subDataset name="beneficiarios" uuid="602272d6-4aea-4d96-ae25-73ea3a87f76b">
		<parameter name="COD_PAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT parcelabeneficiario.idpersona, v_persona.nomcompleto,
    v_persona.tipodocid, v_persona.noidentidad,  codificadores.nombrecodificador as lugarci,
    v_persona.tipoperjuridica, parcelabeneficiario.idparcela, v_persona.pais,
    COALESCE(v_persona.telefono, v_persona.telefono, '') as telefono
   FROM parcelabeneficiario
   JOIN v_persona ON parcelabeneficiario.idpersona = v_persona.idpersona
   JOIN codificadores on codificadores.idcodificadores = CAST(v_persona.lugarci AS INT)
WHERE
parcelabeneficiario.idparcela = $P{COD_PAR} and poblador <> 1]]>
		</queryString>
		<field name="idpersona" class="java.lang.Integer"/>
		<field name="nomcompleto" class="java.lang.String"/>
		<field name="tipodocid" class="java.lang.String"/>
		<field name="noidentidad" class="java.lang.String"/>
		<field name="lugarci" class="java.lang.String"/>
		<field name="tipoperjuridica" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="pais" class="java.lang.String"/>
		<field name="telefono" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Representante" uuid="4ba0de97-e9a2-4789-83a1-772ce1484e4c">
		<parameter name="COD_PAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[select  r.idparcela, v.nomcompleto, COALESCE(r.numdocpoder, r.numdocpoder, '')as numdoc, r.direccionnotificacion, COALESCE(v.telefono, v.telefono, '')as tele, COALESCE(v.email, v.email, '') as mail
from v_persona as v inner join representante as r on v.idpersona = r.idpersona
where r.idparcela = $P{COD_PAR}]]>
		</queryString>
		<field name="idparcela" class="java.lang.String"/>
		<field name="nomcompleto" class="java.lang.String"/>
		<field name="numdoc" class="java.lang.String"/>
		<field name="direccionnotificacion" class="java.lang.String"/>
		<field name="tele" class="java.lang.String"/>
		<field name="mail" class="java.lang.String"/>
	</subDataset>
	<subDataset name="DocumentosPresentados" uuid="2ef95c87-7e18-491b-b7cf-b19f67e7008e">
		<parameter name="COD_PAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[select nombrecodificador, observaciones, idparcela
from docpresentados inner join codificadores on codificadores.idcodificadores = docpresentados.idtipodedocumento
where docpresentados.idparcela= $P{COD_PAR}]]>
		</queryString>
		<field name="nombrecodificador" class="java.lang.String"/>
		<field name="observaciones" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
	</subDataset>
	<subDataset name="InfoPAS" uuid="eac8ad87-2b84-4376-afce-432be44dd58a">
		<parameter name="COD_PAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[select cr.idrechazo, cr.idregistro, COALESCE(cr.observacion,cr.observacion,''), r.idparcela, c.nombrecodificador
from causalesrechazo as cr inner join registro as r on r.idregistro = cr.idregistro inner join codificadores as c on c.idcodificadores = cr.idrechazo
where r.idparcela = $P{COD_PAR} and (cr.idrechazo > 100 or cr.idrechazo < 200)]]>
		</queryString>
		<field name="idrechazo" class="java.lang.Integer"/>
		<field name="idregistro" class="java.lang.Integer"/>
		<field name="coalesce" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="nombrecodificador" class="java.lang.String"/>
	</subDataset>
