	<subDataset name="beneficiarios" uuid="602272d6-4aea-4d96-ae25-73ea3a87f76b">
		<parameter name="COD_PAR" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT parcelabeneficiario.idpersona, v_persona.nomcompleto,
    v_persona.tipodocid, v_persona.noidentidad,  codificadores.nombrecodificador as lugarci,
    v_persona.tipoperjuridica, parcelabeneficiario.idparcela, v_persona.pais,
    COALESCE(v_persona.telefono, v_persona.telefono, '') as telefono
   FROM parcelabeneficiario
   JOIN v_persona ON parcelabeneficiario.idpersona = v_persona.idpersona
   JOIN codificadores on codificadores.idcodificadores = CAST(v_persona.lugarci AS INT)
WHERE
parcelabeneficiario.idparcela = $P{COD_PAR} and poblador <> 1]]>
		</queryString>
		<field name="idpersona" class="java.lang.Integer"/>
		<field name="nomcompleto" class="java.lang.String"/>
		<field name="tipodocid" class="java.lang.String"/>
		<field name="noidentidad" class="java.lang.String"/>
		<field name="lugarci" class="java.lang.String"/>
		<field name="tipoperjuridica" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="pais" class="java.lang.String"/>
		<field name="telefono" class="java.lang.String"/>
	</subDataset>
	<subDataset name="via" uuid="bb25fe98-6fdc-4ae5-81ed-1a0ac258ed22">
		<parameter name="oficina" class="java.lang.String"/>
		<queryString>
			<![CDATA[select c.*, f.*, (((((COALESCE(p.nombre1, ''::bpchar)::text || ' '::text) || COALESCE(p.nombre2, ''::bpchar)::text) || ' '::text) || COALESCE(p.apellidopat, ''::bpchar)::text) || ' '::text) || COALESCE(p.apellidomat, ''::bpchar)::text AS nomcompleto
from funcionario as f inner join persona as p on p.idpersona = f.idpersona
inner join codificadores as c on c.idcodificadores = f.oficina
where nombrecodificador = $P{oficina} and estadofun = 'A' and nombrecargo like '%RESPONSABLE DEPARTAMENTAL%' and (financiamiento = 'UCAB' or financiamiento = 'VDRA')]]>
		</queryString>
		<field name="idcodificadores" class="java.lang.Integer"/>
		<field name="idclasificador" class="java.lang.Integer"/>
		<field name="nombrecodificador" class="java.lang.String"/>
		<field name="orden" class="java.lang.Integer"/>
		<field name="idfuncionario" class="java.lang.Integer"/>
		<field name="idpersona" class="java.lang.Integer"/>
		<field name="fechainiciof" class="java.sql.Date"/>
		<field name="fechafinalf" class="java.sql.Date"/>
		<field name="estadofun" class="java.lang.String"/>
		<field name="login" class="java.lang.String"/>
		<field name="cargo" class="java.lang.Integer"/>
		<field name="nombrecargo" class="java.lang.String"/>
		<field name="idunidad" class="java.lang.Integer"/>
		<field name="financiamiento" class="java.lang.String"/>
		<field name="oficina" class="java.lang.Integer"/>
		<field name="nomcompleto" class="java.lang.String"/>
	</subDataset>
	<parameter name="idregistro" class="java.lang.Integer"/>
	<parameter name="idmonitoreo" class="java.lang.Integer"/>
	<parameter name="anomonitoreo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[-- datos parcela
select * from
(SELECT v_parcela.idregistro, v_parcela.nombre AS nombrepredio, v_parcela.idparcela, v_parcela.departamento, v_parcela.provincia, v_parcela.nombrepolitico AS municipio,
  v_parcela.clasificacionprog, v_parcela."TipoProp", v_parcela."SitJur", v_parcela.estado, v_parcela.asesoramiento, v_parcela.fecharegistro,
  v_parcela.superficie AS suppredio, v.supdefilegal, v.suppas, fr.fechasuscripcion
   FROM v_parcela
   LEFT JOIN v_registro v ON v_parcela.idregistro = v.idregistro
   LEFT JOIN superficiealimentos sa ON sa.idregistro = v_parcela.idregistro
   FULL JOIN fechasregistro fr ON fr.idregistro = v_parcela.idregistro
where v_parcela.idregistro = $P{idregistro}) as a
inner join
(select m.idregistro, m.idmonitoreo, m.anho, m.tipo, m.numeroinforme, m.fechainforme, c.nombrecodificador as oficinainforme,
f.nombrecargo, (((((COALESCE(p.nombre1, ''::bpchar)::text || ' '::text) || COALESCE(p.nombre2, ''::bpchar)::text) || ' '::text) || COALESCE(p.apellidopat, ''::bpchar)::text) || ' '::text) || COALESCE(p.apellidomat, ''::bpchar)::text AS nomcompleto
from monitoreo.funcionariomonitoreo as fm inner join funcionario as f on fm.idfuncionario = f.idfuncionario
inner join persona as p on p.idpersona = f.idpersona
inner join monitoreo.monitoreo as m on m.idmonitoreo = fm.idmonitoreo
inner join codificadores as c on c.idcodificadores = m.oficina
where m.idmonitoreo = $P{idmonitoreo} and tiporegistrador = 138 and m.tipo = 267) as b
on a.idregistro = b.idregistro]]>
	</queryString>
	<field name="idregistro" class="java.lang.Integer"/>
	<field name="nombrepredio" class="java.lang.String"/>
	<field name="idparcela" class="java.lang.String"/>
	<field name="departamento" class="java.lang.String"/>
	<field name="provincia" class="java.lang.String"/>
	<field name="municipio" class="java.lang.String"/>
