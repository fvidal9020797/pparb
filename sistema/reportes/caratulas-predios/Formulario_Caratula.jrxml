	<subDataset name="beneficiarios" uuid="b09fe359-a942-49aa-826b-3603d179ed84">
		<parameter name="codigoparcela" class="java.lang.String"/>
		<queryString>
			<![CDATA[select pb.idparcela, cpb.cuentadeidbeneficiario,
       CASE WHEN cpb.cuentadeidbeneficiario > 2 THEN ((((((COALESCE(persona.nombre1, ''::bpchar)::text || ' '::text) || COALESCE(persona.nombre2, ''::bpchar)::text) || ' '::text) || COALESCE(persona.apellidopat, ''::bpchar)::text) || ' '::text) || COALESCE(persona.apellidomat, ''::bpchar)::text) || ' Y OTROS'::text
            WHEN cpb.cuentadeidbeneficiario <= 2 THEN ((((((COALESCE(persona.nombre1, ''::bpchar)::text || ' '::text) || COALESCE(persona.nombre2, ''::bpchar)::text) || ' '::text) || COALESCE(persona.apellidopat, ''::bpchar)::text) || ' '::text) || COALESCE(persona.apellidomat, ''::bpchar)::text)
            ELSE ''
       END as beneficiario
from parcelabeneficiario as pb inner join persona on pb.idpersona = persona.idpersona
inner join cuenta_parcela_beneficiario as cpb ON pb.idparcela = cpb.idparcela
where poblador < 1 and pb.idparcela = $P{codigoparcela}

order by pb.idparcela asc, persona.idgenero desc
limit 2]]>
		</queryString>
		<field name="idparcela" class="java.lang.String"/>
		<field name="cuentadeidbeneficiario" class="java.lang.Long"/>
		<field name="beneficiario" class="java.lang.String"/>
	</subDataset>
