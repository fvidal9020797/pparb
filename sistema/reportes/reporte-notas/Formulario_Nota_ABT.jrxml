	<subDataset name="detallenota" uuid="aa7daed2-3330-4646-ac8d-a2e273226968">
		<parameter name="idnota" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select d.idnota,r.nombrepredio, r.idparcela, COALESCE(d.observacionregistro,'') as observacionregistro, r.fechasuscripcion, r."Oficina"
from detallenota as d inner join r_general as r on d.idregistro = r.idregistro
where idnota = $P{idnota};]]>
		</queryString>
		<field name="idnota" class="java.lang.Integer"/>
		<field name="nombrepredio" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="observacionregistro" class="java.lang.String"/>
		<field name="fechasuscripcion" class="java.sql.Date"/>
		<field name="Oficina" class="java.lang.String"/>
	</subDataset>
	<subDataset name="cantidadcarpetas" uuid="5ea55dca-5529-48fd-b4f7-6d973fc1f3e1">
		<parameter name="IDNOTA" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select count (idnota) as cantidadnota
from detallenota
where idnota = $P{IDNOTA};]]>
		</queryString>
		<field name="cantidadnota" class="java.lang.Long"/>
	</subDataset>
	<parameter name="ParametroIdNota" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT n.idnota, n.idunidad, n.idremitente, n.iddestinatario, UPPER(n.nnota) as nnota, n.obsnota, n.fechanota, n.fecharegnota, n.fecharecibido, n.idvia, n.idrecepcionado,
TRIM( COALESCE(p.nombre1, '')) || ' ' || TRIM( COALESCE(p.nombre2, '')) || ' ' || TRIM( COALESCE(p.apellidopat, '')) || ' ' || TRIM( COALESCE(p.apellidomat, '')) as remitente, rem.nombrecargo as cargoremitente,
TRIM( COALESCE(p2.nombre1, '')) || ' ' || TRIM( COALESCE(p2.nombre2, '')) || ' ' || TRIM( COALESCE(p2.apellidopat, '')) || ' ' || TRIM( COALESCE(p2.apellidomat, '')) as destinatario, dest.nombrecargo as cargodesitnatario,
TRIM( COALESCE(p3.nombre1, '')) || ' ' || TRIM( COALESCE(p3.nombre2, '')) || ' ' || TRIM( COALESCE(p3.apellidopat, '')) || ' ' || TRIM( COALESCE(p3.apellidomat, '')) as via, COALESCE(via.nombrecargo,'') as cargovia
from    nota as n inner join funcionario as rem on n.idremitente = rem.idfuncionario
	inner join funcionario as dest on n.iddestinatario=dest.idfuncionario
	inner join persona as p on rem.idpersona = p.idpersona inner join persona as p2 on dest.idpersona = p2.idpersona
	left join funcionario as via on n.idvia= via.idfuncionario
	left join persona as p3 on via.idpersona = p3.idpersona
where idnota = $P{ParametroIdNota}]]>
	</queryString>
