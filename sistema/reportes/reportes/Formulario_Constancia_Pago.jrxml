	<subDataset name="PlanPago" uuid="eb7efcb0-b4a0-401b-bd5a-7eda15c901ab">
		<parameter name="P2" class="java.lang.String"/>
		<queryString>
			<![CDATA[select pp.*,c.*, r.idparcela, v.valor_ufv as valorufv, v.fecha_ufv,
(pp.montototal * v.valor_ufv) as montototalbs
from planpago as pp inner join codificadores as c on pp.idtipopago = c.idcodificadores
inner join registro as r on pp.idregistro = r.idregistro
inner join valoresufv v on pp.idvalorufv = v.idvalorufv
where r.idparcela = $P{P2}]]>
		</queryString>
		<field name="idplanpago" class="java.lang.Integer"/>
		<field name="idregistro" class="java.lang.Integer"/>
		<field name="idvalorufv" class="java.lang.Integer"/>
		<field name="montoinicial" class="java.lang.Double"/>
		<field name="montoacuenta" class="java.lang.Double"/>
		<field name="fechapreliquidacion" class="java.sql.Date"/>
		<field name="idtipopago" class="java.lang.Integer"/>
		<field name="cuotas" class="java.lang.Integer"/>
		<field name="montototal" class="java.lang.Double"/>
		<field name="otrosingresos" class="java.lang.Double"/>
		<field name="montoxcuota" class="java.lang.Double"/>
		<field name="idcodificadores" class="java.lang.Integer"/>
		<field name="idclasificador" class="java.lang.Integer"/>
		<field name="nombrecodificador" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="valorufv" class="java.lang.Double"/>
		<field name="fecha_ufv" class="java.sql.Date"/>
		<field name="montototalbs" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="propietarios" uuid="82102815-9a40-47b7-860d-d9dfe86142c4">
		<parameter name="P_pro" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT parcelabeneficiario.idpersona, v_persona.nomcompleto,
    v_persona.tipodocid, v_persona.noidentidad, codificadores.nombrecodificador as lugarci,
    v_persona.tipoperjuridica, parcelabeneficiario.idparcela, v_persona.pais
   FROM parcelabeneficiario
   JOIN v_persona ON parcelabeneficiario.idpersona = v_persona.idpersona
   JOIN codificadores on codificadores.idcodificadores = CAST(v_persona.lugarci AS INT)
WHERE
parcelabeneficiario.idparcela = $P{P_pro}
limit 5]]>
		</queryString>
		<field name="idpersona" class="java.lang.Integer"/>
		<field name="nomcompleto" class="java.lang.String"/>
		<field name="tipodocid" class="java.lang.String"/>
		<field name="noidentidad" class="java.lang.String"/>
		<field name="lugarci" class="java.lang.String"/>
		<field name="tipoperjuridica" class="java.lang.String"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="pais" class="java.lang.String"/>
	</subDataset>
	<subDataset name="PagoRealizado" uuid="c86a6995-4b22-4662-b429-99765b05f240">
		<parameter name="P_plan" class="java.lang.String"/>
		<parameter name="P_idpago" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select pr.*, r.idparcela, eb.nombreentidad, eb.numerocuenta, vu.valor_ufv
from pagosrealizados as pr inner join registro as r on pr.idregistro = r.idregistro
inner join entidadbancaria as eb on pr.identidadbancario = eb.identidadbancario
inner join valoresufv vu on pr.fechadeposito = vu.fecha_ufv
where r.idparcela = $P{P_plan}  and pr.idpago = $P{P_idpago};]]>
		</queryString>
		<field name="idpago" class="java.lang.Integer"/>
		<field name="idregistro" class="java.lang.Integer"/>
		<field name="numerocuota" class="java.lang.Integer"/>
		<field name="identidadbancario" class="java.lang.Integer"/>
		<field name="nboleta" class="java.lang.String"/>
		<field name="montodeposito" class="java.lang.Double"/>
		<field name="fechadeposito" class="java.sql.Date"/>
		<field name="idrecibo" class="java.lang.String"/>
		<field name="fecharegistropago" class="java.sql.Date"/>
		<field name="idparcela" class="java.lang.String"/>
		<field name="nombreentidad" class="java.lang.String"/>
		<field name="numerocuenta" class="java.lang.String"/>
		<field name="valor_ufv" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="PagoRealizado_1" uuid="c86a6995-4b22-4662-b429-99765b05f240">
		<parameter name="P_plan" class="java.lang.String"/>
		<parameter name="P_idpago" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select cuota.fechacuota,
 round((cuota.montoxcuota)::numeric(10,4), 4) AS montoxcuota,
registro.idregistro,
cuota.numerocuota
from
planpago, cuota,registro
where
cuota.idplanpago= planpago.idplanpago and
planpago.idregistro= registro.idregistro and
registro.idparcela=$P{P_plan} and
 not EXISTS(select pagosrealizados.idregistro from pagosrealizados
            where pagosrealizados.idregistro = planpago.idregistro and
            pagosrealizados.numerocuota = cuota.numerocuota)
                        order by fechacuota asc
           LIMIT(1) ;]]>
		</queryString>
		<field name="fechacuota" class="java.sql.Date"/>
		<field name="montoxcuota" class="java.math.BigDecimal"/>
		<field name="idregistro" class="java.lang.Integer"/>
		<field name="numerocuota" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="fechaComprobante" uuid="a30e5e22-cd99-4b6b-8024-22806abf19d0">
		<parameter name="Idpago" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select pr.fecharegistropago
from pagosrealizados as pr
where  pr.idpago = $P{Idpago};]]>
		</queryString>
		<field name="fecharegistropago" class="java.sql.Date"/>
	</subDataset>
	<parameter name="CODIGO_PARCELA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Num_Recibo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT parcelas.idparcela, parcelas.superficie, v_politico.provincia AS PROV, v_politico.departamento AS DPTO,
    v_politico.nombrepolitico as Municipio, c.nombrecodificador AS "TipoProp",
    c1.nombrecodificador AS "SitJur", c2.nombrecodificador AS "Clasificacion",
    c2.nombrecodificador AS idclasificacion, parcelas.nombre,
    c3.nombrecodificador AS "Oficina", c4.nombrecodificador AS estado,
    registro.fecharegistro, registro.idregistro
   FROM v_politico, parcelas, registro, codificadores c, codificadores c1,
    codificadores c2, codificadores c3, codificadores c4
  WHERE v_politico.cod = parcelas.idpolitico AND parcelas.idparcela = registro.idparcela AND parcelas.idtipoprop = c.idcodificadores AND parcelas.idsituacionjuridica = c1.idcodificadores AND parcelas.idclasificacion = c2.idcodificadores AND c3.idcodificadores = registro.idoficina AND registro.estado = c4.idcodificadores AND
     parcelas.idparcela = $P{CODIGO_PARCELA}]]>
	</queryString>
	<field name="idparcela" class="java.lang.String"/>
	<field name="superficie" class="java.lang.Double"/>
	<field name="prov" class="java.lang.String"/>
	<field name="dpto" class="java.lang.String"/>
	<field name="municipio" class="java.lang.String"/>
	<field name="TipoProp" class="java.lang.String"/>
	<field name="SitJur" class="java.lang.String"/>
	<field name="Clasificacion" class="java.lang.String"/>
	<field name="idclasificacion" class="java.lang.String"/>
	<field name="nombre" class="java.lang.String"/>
	<field name="Oficina" class="java.lang.String"/>
	<field name="estado" class="java.lang.String"/>
	<field name="fecharegistro" class="java.sql.Date"/>
	<field name="idregistro" class="java.lang.Integer"/>
