	<subDataset name="predioscampo" uuid="ebeb7fe1-2c04-4b13-8950-f8f78fa7025b">
		<parameter name="idmuestreo" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
'' as num,
r."ID PARCELA",
	r."NOMBRE PREDIO",
	r."MUNICIPIO",
	r."ACTIVIDAD",
	r."ASESORAMIENTO",
	r."SUP TOTAL",
	r."SUP DEFORESTADA ILEGAL" + r."SUP PAS" as SupRegularizada,
	r."REPRESENTANTE NOMBRE",
	r."REPRESENTANTE DIRECCION",
	coalesce(r."REPRESENTANTE TELEFONO",'') as "REPRESENTANTE TELEFONO",
	mr.ponderacion
FROM report_general r,
    muestreo_registro mr
WHERE
     mr.registroid = r."ID REGISTRO"
     AND mr.muestreoid =$P{idmuestreo}
	 and  mr.seleccionado=1
order by r."NOMBRE PREDIO" asc]]>
		</queryString>
		<field name="num" class="java.lang.String"/>
		<field name="ID PARCELA" class="java.lang.String"/>
		<field name="NOMBRE PREDIO" class="java.lang.String"/>
		<field name="MUNICIPIO" class="java.lang.String"/>
		<field name="ACTIVIDAD" class="java.lang.String"/>
		<field name="ASESORAMIENTO" class="java.lang.String"/>
		<field name="SUP TOTAL" class="java.lang.Double"/>
		<field name="supregularizada" class="java.math.BigDecimal"/>
		<field name="REPRESENTANTE NOMBRE" class="java.lang.String"/>
		<field name="REPRESENTANTE DIRECCION" class="java.lang.String"/>
		<field name="REPRESENTANTE TELEFONO" class="java.lang.String"/>
		<field name="ponderacion" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="prediosbeneficiarios" uuid="ebeb7fe1-2c04-4b13-8950-f8f78fa7025b">
		<parameter name="idmuestreo" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT r."ID PARCELA",
	r."NOMBRE PREDIO",
	r."MUNICIPIO",
	r."ACTIVIDAD",
	r."ASESORAMIENTO",
	r."SUP TOTAL",
	r."SUP DEFORESTADA ILEGAL" + r."SUP PAS" as SupRegularizada,
	v.nomcompleto as beneficiario,
	v.direcciondom,
	coalesce(v.telefono,'') telefono
FROM report_general r,
    muestreo_registro mr,
    parcelabeneficiario pb,
    v_persona v
WHERE
     mr.registroid = r."ID REGISTRO"
     AND mr.muestreoid =$P{idmuestreo}
	 and  mr.seleccionado=1
	 and pb.idparcela = r."ID PARCELA"
	 and v.idpersona = pb.idpersona
	and pb.poblador <> 1
order by r."NOMBRE PREDIO" asc]]>
		</queryString>
		<field name="ID PARCELA" class="java.lang.String"/>
		<field name="NOMBRE PREDIO" class="java.lang.String"/>
		<field name="MUNICIPIO" class="java.lang.String"/>
		<field name="ACTIVIDAD" class="java.lang.String"/>
		<field name="ASESORAMIENTO" class="java.lang.String"/>
		<field name="SUP TOTAL" class="java.lang.Double"/>
		<field name="supregularizada" class="java.math.BigDecimal"/>
		<field name="beneficiario" class="java.lang.String"/>
		<field name="direcciondom" class="java.lang.String"/>
		<field name="telefono" class="java.lang.String"/>
	</subDataset>
	<parameter name="report_parametro_1" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select mt.*,ms.totalmuestra from (select m.*,t.totalpredios from (SELECT mu.id, mu.descripcion, rtrim(p.nombrepolitico) as departamento, mu.anho, c.nombrecodificador as tipo, mu.fecha, mu.estado, mu.elinimado,mu.n, mu.k, mu.xxmin, mu.xxmax, mu.r, mu.l
  FROM muestreo mu,codificadores c ,politico p where c.idcodificadores=mu.tipo and p.idpolitico=mu.departamento and estado=1 and elinimado isnull and id= $P{report_parametro_1})as m left join
  (SELECT muestreoid,count(muestreoid) as totalpredios from muestreo_registro group by muestreoid)as t on m.id=t.muestreoid)as mt left join
  (SELECT muestreoid,count(muestreoid) as totalmuestra from muestreo_registro where seleccionado=1 group by muestreoid)as ms on mt.id=ms.muestreoid  order by id]]>
	</queryString>
	<field name="id" class="java.lang.Integer"/>
	<field name="descripcion" class="java.lang.String"/>
	<field name="departamento" class="java.lang.String"/>
	<field name="anho" class="java.lang.Integer"/>
	<field name="tipo" class="java.lang.String"/>
	<field name="fecha" class="java.sql.Date"/>
	<field name="estado" class="java.lang.Integer"/>
	<field name="elinimado" class="java.sql.Date"/>
	<field name="n" class="java.lang.Integer"/>
	<field name="k" class="java.lang.Integer"/>
	<field name="xxmin" class="java.lang.Double"/>
	<field name="xxmax" class="java.lang.Double"/>
	<field name="r" class="java.lang.Double"/>
	<field name="l" class="java.lang.Double"/>
	<field name="totalpredios" class="java.lang.Long"/>
	<field name="totalmuestra" class="java.lang.Long"/>
